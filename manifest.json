{
  "manifest_version": 2,
  "name": "AllDebrid for desktop",
  "description": "alternative interface for AllDebrid service",
  "version": "0.6.0",
  "minimum_chrome_version": "31",
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "permissions": [
    "http://*.alldebrid.com/*",
    "storage"
  ],
  "short_name": "ad-chrome-dev"
}
